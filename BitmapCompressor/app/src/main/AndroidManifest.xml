<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.seekting.compressor"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.seekting.compressor.DemoSyncLaunchActivity"/>
        <activity android:name="com.seekting.compressor.DemoAsyncLaunchActivity"/>
        <activity android:name="com.seekting.compressor.DemoLaunchWithCallBack"/>
        <activity android:name="com.seekting.compressor.DemoErrorLaunchActivity"/>
        <activity android:name="com.seekting.compressor.CustomCompressActivity"/>

    </application>

</manifest>